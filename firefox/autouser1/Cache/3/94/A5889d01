		});

function ignoreData() { }

function trackMoreLink() {
    s.prop18='More Link';
	s.eVar10='More Link';
	s.events='event30';
	void(s.t());
	s.prop18='';
	s.eVar10='';
	s.events='';
}

function trackPromoLink(obj, country_Code) {
    var s=s_gi(s_account); //Not sure if this is really necessary;
    s.linkTrackVars='events,eVar19, prop3, eVar1';
    s.linkTrackEvents='event31';
    s.events='event31';
    s.eVar1='internal campaign';
    s.eVar19=country_Code;
    s.prop3=country_Code;

    s.tl(obj,'o','on-site promo');
}

// Function to display right pane
function displayRightTabPane(leftID) {

	alert($(leftID).html());
}

// Login Box - Pre Active State
function activateLogin() {
	$("#login-active").animate( {
		marginTop : '-42px'
	}, 'slow');
}

function deactivateLogin() {
	$("#login-active").animate( {
		marginTop : '-235px'
	}, 'slow');
}

// Secondary Nav Drop Down Function
function toggleDropdown(dom) {
	if ($(dom).find('.dropdown:first').css('display') == 'none') {
		$(dom).find('.dropdown:first').stop(true, true).fadeIn();
	} else {
		$(dom).find('.dropdown:first').stop(true, true).fadeOut();
	}
}

// Widget Toggling Function
function toggleWidget(dom) {
	if ($(dom).hasClass('collapsed')) {
		$(dom).removeClass('collapsed');
		$(dom).addClass('expanded');
	} else {
		$(dom).removeClass('expanded');
		$(dom).addClass('collapsed');
	}
	$(dom).parent().find('.widget-content').toggle();
}

// Swap Tab Subcontent
function loadSubcontent(caller, dom) {
	$("#tabs .subnav a.subnav-item").removeClass("selected");
	$(caller).addClass("selected");
	$('.subcontent div').hide();
	$(dom).show();
}

// Tab generation functions

function generateAllTabTitlesInTarget(currentTabWrapper, target) {

	var titles = $(currentTabWrapper).find(tabHeader);
	var titleWrapper_Div_Start = divStart + space + attr_class + equalTo
			+ tab_title_titleWrapperStyle + gt;
	var allTitleDivs = "";
	var noOfTitles = titles.length;
	for ( var index = 0; index < noOfTitles; index++) {
		var currentTitle = titles[index];

		var triggerID = "tab_TitleNo#" + index;
		var title_Div = divStart + space + attr_class + equalTo + dblQuotes
				+ tab_title_Div_Class + space;
		title_Div = title_Div + $(currentTitle).attr(attr_class) + dblQuotes
				+ space;
		title_Div = title_Div + attr_name + equalTo + dblQuotes + triggerID
				+ dblQuotes + gt;
		title_Div = title_Div + currentTitle.innerHTML + divEnd;
		allTitleDivs = allTitleDivs + title_Div;

	}

	return titleWrapper_Div_Start + allTitleDivs + divEnd;

}

function generateTabBodiesInTarget(aggregatedMarkup, currentTabWrapper, target) {
	var tabBodies = $(currentTabWrapper).find(tabBody);
	var tabBodiesAggregatedMarkup = "";
	var noOfTabBodies = tabBodies.length;
	for ( var index = 0; index < noOfTabBodies; index++) {
		var currentTabBody = tabBodies[index];
		var tabBodyMarkup = divStart + space + attr_class + equalTo + dblQuotes
				+ tab_body_Div_Class + space
				+ $(currentTabBody).attr(attr_class) + dblQuotes + gt;
		tabBodyMarkup = tabBodyMarkup
				+ generate2PartitionStruct(currentTabBody);
		tabBodyMarkup = tabBodyMarkup + divEnd;
		tabBodiesAggregatedMarkup = tabBodiesAggregatedMarkup + tabBodyMarkup;
	}

	return aggregatedMarkup + tabBodiesAggregatedMarkup;

}

function generate2PartitionStruct(tabBody) {
	var newBodyMakrup = tabBody.innerHTML;
	var left_Elements = $(tabBody).find(leftSection);
	var right_Elements = $(tabBody).find(rightSection);
	var counter = 0;
	if (left_Elements.length > 0) {
		newBodyMakrup = divStart + space + attr_class + equalTo + dblQuotes
				+ tab_body_Div_Class + dblQuotes + gt;
		// create left partition
		var leftPartition = divStart + space + attr_class + equalTo + dblQuotes
				+ tab_leftSection_Div_Class + dblQuotes + gt;
		for ( var index = 0; index < left_Elements.length; index++) {
			var triggerName = "LeftItemNo#" + index;
			var leftItem = left_Elements[index];
			var leftElement = lt + leftItem.tagName + space + attr_name
					+ equalTo + dblQuotes + triggerName + dblQuotes + space
					+ attr_class + equalTo + dblQuotes + tab_left_Item_Class
					+ space + $(leftItem).attr(attr_class) + dblQuotes + space
					+ gt;
			leftElement = leftElement + leftItem.innerHTML + lt + frwdSlash
					+ leftItem.tagName + gt;
			leftPartition = leftPartition + leftElement;
			counter++;
		}
		leftPartition = leftPartition + divEnd;

		// create right partition
		var rightPartition = divStart + space + attr_class + equalTo
				+ dblQuotes + tab_rightSectionWrapper_Div_Class + dblQuotes
				+ gt;
		for ( var index = 0; index < right_Elements.length; index++) {
			var rightItem = right_Elements[index];
			var rightElement = divStart + space + attr_class + equalTo
					+ dblQuotes + tab_rightSectionItem_Div_Class + space
					+ $(rightItem).attr(attr_class) + dblQuotes + space + gt;

			rightElement = rightElement + rightItem.innerHTML + divEnd;
			rightPartition = rightPartition + rightElement;
		}
		rightPartition = rightPartition + divEnd;
		newBodyMakrup = newBodyMakrup + leftPartition + rightPartition + divEnd;
	}

	return newBodyMakrup;
}

function submitLogin() {
	document.forms["loginForm_new"].submit();
}
function fetchFriendlyUrl(actualUrl, proxySetting, monthVal, contentType) {
	var friendlyUrl;
	friendlyUrl = actualUrl;
	if (proxySetting == "proxy" && !(actualUrl.indexOf("http") == 0)) {
		var indexHome = 0;
		var tempUrl;

		indexHome = actualUrl.indexOf("home");
		if (indexHome != -1) { // has home
			tempUrl = actualUrl.substr(actualUrl.lastIndexOf("home"));
			if (tempUrl.indexOf("index") != -1) {
				tempUrl = tempUrl.substr(0, tempUrl.indexOf("index"));
			}
			friendlyUrl = tempUrl.substr(4);
			if (!(contentType == "CaseStudyContent")) { // appending month value
														// for news & press
														// release contents
				tempUrl = friendlyUrl.substr(friendlyUrl.lastIndexOf("/"));
				friendlyUrl = friendlyUrl.substr(0, friendlyUrl
						.lastIndexOf("/"));
				friendlyUrl = friendlyUrl + "/" + monthVal + tempUrl;
			}

		}
	}
	return friendlyUrl;
}


function cleanCookieValue(cookieValue) {
	if (cookieValue != null) {
		var cookieAsHtml = $.encoder.canonicalize(cookieValue);
		cookieAsHtml = cookieAsHtml.replace(/(\<.*\>)/, "").replace(
				/[\"\'].*[\"\']/, "");
		return cookieAsHtml;
	} else {
		return cookieValue;
	}
}

function updateAddThisWidget(title, image, summary, link) {

	updateFaceBook(title, image, summary, link);
	updateLinkedIn(title, image, summary, link);

}

function updateFaceBook(title, image, summary, link) {
	var finalURI = "http://www.facebook.com/share.php?s=100";
	if (null != title) {
		finalURI += "&p%5Btitle%5D=" + encodeURI(title);
	}
	if (null != image) {
		finalURI += "&p%5Bimages%5D%5B0%5D=" + encodeURI(image)
				+ "&p%5Bimages%5D%5B1%5D=" + encodeURI(image);
	}
	if (null != summary) {
		finalURI += "&p%5Bsummary%5D=" + encodeURI(summary);
	}
	if (null != link) {
		finalURI += "&p%5Burl%5D=" + encodeURI(link);
	}
	$("#atic_facebook").attr("href", finalURI);
	$("#atic_facebook").attr("onclick","");
}

function updateLinkedIn(title, image, summary, link) {
	var finalURI = 'http://www.linkedin.com/shareArticle?mini=true';

	if (null != title) {
		finalURI += '&title=' + encodeURI(title);
	}
	if (null != link) {
		finalURI += '&url=' + encodeURI(link);
	}
	if (null != summary) {
		finalURI += '&summary=' + encodeURI(summary);
	}
	finalURI += '&source=Red Hat official website';
	$("#atic_linkedin").attr("href", finalURI);
	$("#atic_linkedin").attr("onclick","");

}

//Removing html fragment by selector and pattern
function showHideListingBySelector(selector,nonPermissiblePattern,permissiblePattern,showOnPermissible,callback){
	if((location.pathname.match(permissiblePattern) != null) && (location.pathname.match(nonPermissiblePattern)==null)){
		if(showOnPermissible==true){
			$(selector).show();
		}else{
			$(selector).hide();
		}
		if(callback!=null){
			window[callback]();
		}
	}
}
			
function displayTwitterWidget(){
	var s = 'script';
	var id = 'twitter-wjs';
	var d = document;
	var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';
	if(!d.getElementById(id)){
		js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);
	}
}
function hideLessMoreFromJapanPage(){
	if(location.href.substr(location.href.length-7,location.href.length)=="/about/" && getCookieByName("LOCALE")=="ja_JP"){
		$(".lessmoretext").slideToggle("slow");
		$('.showmore').hide();
		$('.lessmore').hide();
		$('.readmore').hide();
		}
			
	}
function getCookieByName(c_name){
	var c_value = document.cookie;
	var c_start = c_value.indexOf(" " + c_name + "=");
	if(c_start == -1){
	  c_start = c_value.indexOf(c_name + "=");
	  }
	if(c_start == -1){
	  c_value = null;
	  }
	else{
	  c_start = c_value.indexOf("=", c_start) + 1;
	  var c_end = c_value.indexOf(";", c_start);
	  if (c_end == -1){
		c_end = c_value.length;
	  }
	c_value = unescape(c_value.substring(c_start,c_end));
	}
	return c_value;
}