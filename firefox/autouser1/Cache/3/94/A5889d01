//Disabling loggings for production
var console = {};
console.log = function(){};
//Omniture Facet Search
var facetSearchVar="";
var facetSearchHierarchyVar="";


//Constants: Should not be modified in code
var divStart = "<div";
var divEnd = "</div>";
var attr_id = "id";
var attr_name = "name";
var attr_class = "class";
var space = " ";
var dblQuotes = "\"";
var equalTo = "=";
var frwdSlash = "/";
var lt = "<";
var gt = ">";

// Constants: Tabbed View CSS Class names used for Jquery css selector to locate
// html elements
var tabWrapper_class = '.tabWrapper';
var tabHeader = '.tabHeader';
var tabItem = '.tabItem';
var tabBody = '.tabBody';
var leftSection = '.left_section';
var rightSection = '.right_section';

// Constants: Tabbed View CSS Class names for providing tabbed styling

var tab_title_titleWrapperStyle = "titleWrapperStyle";
var tab_title_Div_Class = "tabHeader_style";
var tab_title_active_Style = "tabactive";
var tab_title_Hover_Style = "tabactive";

var tab_body_Div_Class = "tabBody_style";
var tab_leftSection_Div_Class = "left_section_style";
var tab_left_Item_Class = "left_section_item_style";
var tab_rightSectionWrapper_Div_Class = "right_section_wrapper_style";
var tab_rightSectionItem_Div_Class = "right_section_item_style";
var tab_leftItemActiveStyle = "active";

// This variable is used in Accordian functionality. If value is true then the
// default behaviour of accordian
// i.e display first drawer - will be skipped
var _cancelAccordianDefault = false;

var isNotLoaded = true;

// Holds the classes of last hovered tab title.
var lastVisited_classes;

var seyMourLabel;
var seyLessLabel;

// holds accordion tooltip to be displayed in expanded state
var accordianExpandToolTip = "";
// holds accordion tooltip to be displayed in collapsed state
var accordianCollapseToolTip = "";
// holds title to be set for like icon in facebook
var facebookLikeTitle = "likeTitle";
var oldPageName;
var oldCountryName;

//Encoding Variable
var immune;

$(document)
		.ready(function() {
			if (isNotLoaded) {
				isNotLoaded = false;
				$("#login-active").remove();
				// Show Hide Section
				showHideListingBySelector(".twitter-show-link",/\/about\/company\/management\/.+/,/\/about\/company\/management\//,true,null);	
				showHideListingBySelector('div#primary-nav a[href="/products/enterprise-linux/scientific-computing/"]',null,/\.*/,false,null);
				showHideListingBySelector('a[href="/products/enterprise-linux/management/"]',null,/\.*/,false,null);
				showHideListingBySelector('div#primary-nav a[href="/products/jbossenterprisemiddleware/integrate/"]',null,/\.*/,false,null);
				showHideListingBySelector('div#primary-nav a[href="/products/jbossenterprisemiddleware/accelerate/"]',null,/\.*/,false,null);
				showHideListingBySelector('div#primary-nav a[href="/products/jbossenterprisemiddleware/automate/"]',null,/\.*/,false,null);
				showHideListingBySelector(".twitter-show-link",/\/about\/company\/management\/.+/,/\/about\/company\/management\//,true,'displayTwitterWidget');		
				/*Change link of Partners Europe on twitter*/
				if(location.pathname=="/community/"){
				$($('div#twitterBlock').find('a')[($('div#twitterBlock').find('a').size()-2)]).attr('href','https://twitter.com/RHTPartnersEMEA');
				}
				/*Add This - Share Widget - Social Media -  fix*/
				var title=document.title;
				var image='http://www.redhat.com/rhecm/rest-rhecm/jcr/repository/collaboration/sites%20content/live/redhat/web-cabinet/static-files/images/ShadowMan214x214.jpg';
				var summary= $('meta[name=description]').attr("content") ;
				var link=document.documentURI;
				if(navigator.appVersion.indexOf('MSIE') >= 0){
							document.getElementById('share_widget_style').setAttribute('addthis:url',location.href);
							document.getElementById('share_widget_style').setAttribute('addthis:uri',location.href);
							document.getElementById('share_widget_style').setAttribute('addthis:title',title);
							document.getElementById('share_widget_style').setAttribute('addthis:description',summary);
							setTimeout(function(){
							$('#share_widget_style').mouseover(function(){setTimeout(function(){
							updateAddThisWidget(title,image,summary,location.href);},200);});
							/*$('.events_container .addthis_toolbox').mouseover(function(){setTimeout(function(){
							updateAddThisWidget(null,image,null,null);},200);});*/
							},1000);
				}else{
					$("#share_widget_style").attr("addthis:url",link).attr("addthis:uri",link).attr("addthis:title",title).attr("addthis:description",summary);	
					setTimeout(function(){
					$('#share_widget_style').mouseover(function(){setTimeout(function(){
					updateAddThisWidget(title,image,summary,link);},200);});
					/*$('.events_container .addthis_toolbox').mouseover(function(){setTimeout(function(){
					updateAddThisWidget(null,image,null,null);},200);});*/
				},1000);				
				}
				
				//login widget css changes
				$('#noScriptMessage').css("display", "none");
				$('#login-active').hide();
					
				// Generating Tabbed view
				$(tabWrapper_class).hide();

				// start constructing tabs
				var allTabWrappers = $(tabWrapper_class);

				var allRenderingPanes = $(tabWrapper_class).next();

				var noOfTabWrappers = allTabWrappers.length;

				for ( var index = 0; index < noOfTabWrappers; index++) {
					var aggregatedMarkup = "";
					aggregatedMarkup = generateAllTabTitlesInTarget(
							allTabWrappers[index], allRenderingPanes[index]);
					aggregatedMarkup = generateTabBodiesInTarget(
							aggregatedMarkup, allTabWrappers[index],
							allRenderingPanes[index]);

					var target = allRenderingPanes[index];
					target.innerHTML = aggregatedMarkup;
					// default: first tab is active
					var firstTabHeaderElement = $(target).find(tabHeader)[0];
					$(firstTabHeaderElement).addClass(tab_title_active_Style);
					// default: Hide all tab bodies;
					$(target).find(tabBody).hide();
					// default: first tab body is to be shown;
					var firstTabBodyElement = $(target).find(tabBody)[0];
					$(firstTabBodyElement).show();

					// default: Hide all right sections
					$(target).find("." + tab_rightSectionItem_Div_Class).hide();
					// default: Show first right section
					var firstRightSectionItem = $(target).find(
							"." + tab_rightSectionItem_Div_Class)[0];
					$(firstRightSectionItem).show();
					var firstLeftSectionItem = $(target).find(leftSection)[0];
					$(firstLeftSectionItem).find("a").addClass(
							tab_leftItemActiveStyle);	

					// Add events
					// event:Clicking of Tab Title displays the
					// corresponding tab body
					$(target)
							.find(tabHeader)
							.click(
									function() {
										var divID = $(this).attr(attr_name);
										var index = divID.split("#")[1];
										var currentTabbedPane = $(this)
												.parent().parent();
										var titleWrapper = $(this).parent();
										$(titleWrapper).find(tabHeader)
												.removeClass(
														tab_title_active_Style);
										$(this)
												.addClass(
														tab_title_active_Style);

										var tabBodies = $(currentTabbedPane)
												.find(tabBody);
										var targetTabBody = tabBodies[index];
										$(currentTabbedPane).find(tabBody)
												.hide();
										$(targetTabBody).show();
										$(targetTabBody)
												.find(leftSection)
												.find("a")
												.removeClass(
														tab_leftItemActiveStyle);
										var leftSectionFirst = $(targetTabBody)
												.find(leftSection)[0];
										var leftSectionFirstFirst = $(leftSectionFirst)[0];
										var leftSectionFirstFirstAnchor = $(
												leftSectionFirstFirst)
												.find("a")
												.addClass(
														tab_leftItemActiveStyle);

										lastVisited_classes = $(this).attr(
												attr_class);
										$(targetTabBody)
												.find(
														"."
																+ tab_rightSectionItem_Div_Class)
												.hide();

										var tabRightSectionFirst = $(
												targetTabBody)
												.find(
														"."
																+ tab_rightSectionItem_Div_Class)[0];
										$(tabRightSectionFirst).show();
										/*Tracking omniture tab click*/
										oldPageName = s.pageName;
										oldCountryName=s.prop5;
										s.prop5=s.prop5+" | "+escape($(this).html()).replace(/%20/g, " ");
										s.eVar25=s.eVar25+" | "+escape($(this).html()).replace(/%20/g, " ");				
										s.pageName = s.pageName + " | "+escape($(this).html()).replace(/%20/g, " ");
										void(s.t());
										s.pageName = oldPageName;
										s.prop5=oldCountryName;
										s.eVar25=oldCountryName;
									});

					// event:On mouse over add a new class
					$(target).find(tabHeader).mouseover(function() {
						lastVisited_classes = $(this).attr(attr_class);
						/* Tab tile:on mouse over style */
						$(this).addClass(tab_title_Hover_Style);

					});

					// event:On mouse out restore the original classes
					$(target).find(tabHeader).mouseout(function() {
						/* Tab tile:on mouse over style */
						$(this).removeClass(tab_title_Hover_Style);
						$(this).addClass(lastVisited_classes);
					});

					// event: Clicking on left section Reveals right
					// section in current tab
					$(target)
							.find(leftSection)
							.click(
									function() {

										var srcName = $(this).attr(attr_name);
										var index = srcName.split("#")[1];
										$(this)
												.parent()
												.find("p a")
												.removeClass(
														tab_leftItemActiveStyle);
										$(this).find("a").addClass(
												tab_leftItemActiveStyle);
										var rightSections = $(this)
												.parent()
												.next()
												.find(
														"."
																+ tab_rightSectionItem_Div_Class);
										var targetSection = rightSections[index];
										$(rightSections).hide();
										$(targetSection).show();
									});
				}
				$('a').each(function() {
					var a = new RegExp('/' + window.location.host + '/');
					//alert(this.href);
					if(!a.test(this.href) && this.href!='' && this.href!=null && this.href!='#' && !(this.href.indexOf('javascript:ajaxGet')>-1) && !(this.href.indexOf('javascript:void')>-1)) {
					$(this).click(function(event) {
						event.preventDefault();
						event.stopPropagation();
					window.open(this.href, '_blank');
					});
					}
					}); 


				// end tabs construction

				// accordian behaviour
				if ($('.accordian_wrapper .accordian_title') != null
						&& $('.accordian_wrapper .accordian_title').length > 0) {
					if (!_cancelAccordianDefault) {
						$('.accordian_wrapper .accordian_title').removeClass(
								"active");
						$('.accordian_wrapper .accordian_title').attr("title",
								accordianCollapseToolTip);
						$('.accordian_wrapper .accordian_title').next().hide();

						var accordianTitleFirstElement = $('.accordian_wrapper .accordian_title')[0];
						$(accordianTitleFirstElement).addClass("active");
						$(accordianTitleFirstElement).attr("title",
								accordianExpandToolTip);
						$(accordianTitleFirstElement).next().show();
						var accordianWrapperFirst = $('.accordian_wrapper')[0];
						var accordianTitleFirst = $(accordianWrapperFirst)
								.find('.accordian_wrapper .accordian_title')[0];
						$(accordianTitleFirst).addClass("active)");
						$(accordianTitleFirst).attr("title",
								accordianExpandToolTip);
						$(accordianTitleFirst).next().show();

					}
					$('.accordian_wrapper .accordian_title')
							.click(
									function() {
										if (!$(this).hasClass("active")) {
											var targetItem = $(this).parent()
													.find('.accordian_title');
											$(targetItem).removeClass("active");
											$(targetItem).attr("title",
													accordianCollapseToolTip);
											$(this).parent().find(
													'.accordian_title').next()
													.hide();

											$(this).addClass("active");
											$(this).attr("title",
													accordianExpandToolTip);
											$(this).next().show();
											var subFirstAccordianTitle = $(this)
													.next()
													.find(
															'.accordian_wrapper .accordian_title')[0];
											$(subFirstAccordianTitle).addClass(
													"active");
											$(subFirstAccordianTitle).attr(
													"title",
													accordianExpandToolTip);
											$(subFirstAccordianTitle).next()
													.show();
										} else {
											$(this).parent().find(
													'.accordian_title')
													.removeClass("active");
											$(this).parent().find(
													'.accordian_title').attr(
													"title",
													accordianCollapseToolTip);
											$(this).parent().find(
													'.accordian_title').next()
													.hide();
										}

									});

				}
				// end accordian behaviour

				// See More Function
				$('.seymour').next().hide();
				$('.seymour')
						.click(
								function() {

									$(this).toggleClass("active").next()
											.slideToggle("slow");

									var classArray = $(this).attr("class")
											.split(" ");
									var foundActive = false;
									for ( var counter = 0; counter < classArray.length; counter++) {
										if (classArray[counter] == "active") {
											$(this).html(seyLessLabel);
											foundActive = true;
										}
									}
									if (!foundActive) {
										$(this).html(seyMourLabel);
									}

								});

				// Read More Section
				if ($('.lessmore').length > 0) {
					if ($('.lessmore').prev()[0].tagName.toUpperCase() == 'P') {
						$('.lessmore').appendTo($('.lessmore').prev());
					} else {

						$('.lessmore').prev().parent().append("<p></p>")
						$('.lessmore').appendTo($('.lessmore').next());

					}
				}
				if ($('.readmore').length > 0) {
					if ($('.readmore').prev()[0].tagName.toUpperCase() == 'P') {
						$('.readmore').appendTo($('.readmore').prev());
					} else {
						$('.readmore').prev().parent().append("<p></p>");
						$('.readmore').appendTo($('.readmore').next());
					}
				}
				$('.lessmoretext').hide();
				$('.readmore').click(
						function() {

							$(this).hide();
							$(this).toggleClass("active").parent().parent(
									".readmoretext").nextAll(".lessmoretext")
									.slideToggle("slow");
							trackMoreLink();
						});

				$('.lessmore').click(
						function() {
							$(this).toggleClass("active").parent().parent(
									".lessmoretext").slideToggle("slow");

							$(this).parent().parent(".lessmoretext").parent()
									.find("div.readmoretext").find(".readmore")
									.show();
						});
						
				// white space removal starts here(Defect- DE3193)
				var finalData="";
				var data1=$('div.lessmoretext').children();
			
				for(var count=0;count<data1.length-1;count++){
					var data=$($(data1)[count]).text().trim();
					finalData=finalData+data;
					if(data.length>0){
						break;
					}
				}
				if(finalData.length==0){				
				var singleElemData=$($(data1)[data1.length-1]).text().trim();
				var localisedLessLableText=$('div.lessmoretext').find('.lessmore').text().trim();
				if(singleElemData==localisedLessLableText){
				finalData="";
				}else{
				finalData=singleElemData;
				}
				}
			
				if(finalData.length==0){
					$('span.readmore').css("display", "none");
				}
				
			
				// white space removal ends here
				
				// Search Box 'Type To Search' switch
				$('.search-field').focus(function() {
					if (this.value == this.defaultValue) {
						this.value = '';
					}
					if (this.value != this.defaultValue) {
						this.select();
					}
				});

				// Footer Call Out Box Hover Animations
				$('#call-out-boxes div').hover(function() {
					$(this).find('img').stop().animate( {
						marginLeft : '-156px'
					}, 'slow');

				}, function() {
					$(this).find('img').stop().animate( {
						marginLeft : '0px'
					}, 'slow');
				});
				// Check for secondary nav dropdowns

				// Widget Toggling
				if ($("#content .widget .widget-toggler").length > 0) {
					$("#content .widget .widget-toggler").click(function() {
						toggleWidget(this);
					});
				}

			
			
			
				// Tabs
				if ($("#tabs").length > 0) {
					if ($("#product-detail").length > 0) {
						$('#tabs').tabs( {
							selected : 2
						});
					} else if ($("#solutions").length > 0) {
						$('#tabs').tabs( {
							selected : 1
						});
					} else {
						$('#tabs').tabs();
					}
				}

				// Product Boxes
				if ($(".product-box").length > 0) {
					$('.product-box').hover(function() {
						$(this).addClass('active');
					}, function() {
						$(this).removeClass('active');
					});

					$('.product-box').click(function() {
						if ($(this).attr("rel") != null) {
							window.location = $(this).attr("rel");
						}

					});
				}
				// Secondary navigation
				$('#showCountryLang_a').click(function() {
					$('#countryLanguage_OverLay').css("display", "block");
				});

				$('#hideCountryLang_a').click(function() {
					$('#countryLanguage_OverLay').css("display", "none");
				});

				/* widget styling */
				var widget_portlet = $('#sidebar-right .widget_portlet');
				var noOfWidgets = $(widget_portlet).length;
				var lastWidget = $(widget_portlet)[noOfWidgets - 1];
				if (widget_portlet.length > 1) {
					var firstWidget = $(widget_portlet)[0];
					$(firstWidget).addClass("firstWidgetStyle");
					$(lastWidget).addClass("lastWidgetStyle");
				} else {
					$(lastWidget).addClass("oneWidgetStyle");
				}

				// for Right hand widgets panel open close
				$(".widget-detail").hide();
				$(".widget_portlet h5").attr("title", accordianCollapseToolTip);
				$(".widget_portlet h5").click(function() {
					$(this).toggleClass("active").next().slideToggle("slow");

					if ($(this).hasClass("active")) {
						$(this).attr("title", accordianExpandToolTip);
					} else {
						$(this).attr("title", accordianCollapseToolTip);
					}

				});

				// For testimonial region selection drop down functionality
				var firstChildOfSiblingOfRegionNode = $(
						"#regionSelection_input").next().children()[0];
				$(firstChildOfSiblingOfRegionNode).children().click(function() {
					submitTestimonialForm();
				});

				// Select Box Styling
				//$('.container select').selectbox( {
				$('.container select:not([name=\'site\'])').selectbox( {
					debug : false
				});
				
				// for scrolling function of dropdown to work properly on user click
				$('.selectbox-wrapper ul li').click(
				function(){
					var currentStyle=$(this).parent().parent().attr('style');
					$(this).parent().parent().attr('style','display:none !important');
					$(this).parent().parent().attr('style',currentStyle);
				}		
				);				

				// Adding # when javascript is enabled
				$('#showCountryLang_a').attr('href', '#');

				// Type to search

				var defaultSearchBoxVal = $("#searchBox").attr('value');
				$('#searchBox').blur(function() {
					var newValue = $("#searchBox").attr('value');
					if (newValue == '') {
						$("#searchBox").attr('value', defaultSearchBoxVal);
					}
				});

				// quicklinks redirection
				$('#quick_links_form #_container li').click(function() {
					var gotoAddr = $(this).attr("id").split("_input_")[1];
					gotoAddr = gotoAddr.trim();
					if (gotoAddr == "") {
						return;
					} else {
						var tokens=gotoAddr.split("|");
						if(tokens[0]=="e"){
						window.open(tokens[1]);
						}else{
							window.location.href = tokens[1];
						}
					}
				});
				
				//Track promotions
				$('#sidebar-right .promo-portlet a').click(function(){				
					trackPromoLink(this,s.prop3);
				});
				
				//Track Homepage promotions
				$('#innerdiv a#home-page-image_Link').click(function(){
					
					trackPromoLink(this,s.prop3);
				});
				
				/*added code to hide more/less functionality from /about/ in japanese locale*/			
			hideLessMoreFromJapanPage();
				
			}
             
						 
			//Omnibug event for resource library facet search filter click
		//	$('div.fs_nav_portlet div#searchFormContainer div.resource_panel_detail div.section p').click(function(){
				//alert("inside");
			//	s.events="event16";
			//	void(s.t());
			//});
			var myKey = "223190404d28f4fcabacfadefef244ea33868fb1";
			var demandbasescript = document.createElement('script');
			demandbasescript.src = "//api.demandbase.com/api/v2/ip.json?key="+myKey+"&page="+document.location.href+"&page_title="+document.title+"&referrer="+document.referrer+"&callback=ignoreData";
			document.getElementsByTagName('head')[0].appendChild(demandbasescript);
			
			// Google Remarketing //
			var googleImg = document.createElement('img');
			googleImg.src = "//googleads.g.doubleclick.net/pagead/viewthroughconversion/990030321/?value=0&label=rxV4CN_35QQQ8dOK2AM&guid=ON&script=0";
			document.getElementsByTagName('head')[0].appendChild(googleImg);
			
			/*if(location.pathname=='/about/company/offices.html'){
			$("html").attr("xmlns:og","http://ogp.me/ns#")
